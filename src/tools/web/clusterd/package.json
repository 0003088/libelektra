{
  "name": "@elektra-web/clusterd",
  "version": "1.0.0",
  "description": "server to control (multiple) elektrad instances",
  "keywords": ["elektra", "elektra-web", "clusterd"],
  "homepage": "http://git.libelektra.org/blob/master/src/tools/web/",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "cd ../client && npm install",
    "start": "npm run build && npm run start:prod",
    "start:verbose": "npm run build && npm run start:prod:verbose",
    "build": "babel -d build/ src/ && cd ../client && npm run build",
    "start:prod": "cross-env NODE_ENV=production DEBUG=clusterd:*,-*debug,-*log node build/",
    "start:prod:verbose": "cross-env NODE_ENV=production DEBUG=clusterd:* node build/",
    "start:dev": "cross-env NODE_ENV=development DEBUG=clusterd:* babel-watch src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "babel-loader": "^6.2.5",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "debug-dude": "^1.0.3",
    "express": "^4.14.0",
    "node-fetch": "^1.6.1",
    "node-uuid": "^1.4.7",
    "cross-env": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "babel-watch": "^2.0.2"
  }
}
